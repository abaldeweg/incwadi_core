{
  "name": "baldeweg/incwadi-core",
  "description": "incwadi is a book database to manage a lot of books.",
  "type": "project",
  "license": "MIT",
  "require": {
    "baldeweg/pack": "^1.0",
    "symfony/flex": "^1.1"
  },
  "require-dev": {
    "baldeweg/dev-pack": "^1.0",
    "doctrine/doctrine-fixtures-bundle": "^3.1"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Baldeweg\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Baldeweg\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C1Q8G530GEWSFB5YPMD92K2C",
      "allow-contrib": true,
      "require": "~4.2"
    }
  }
}
